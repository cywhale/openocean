{
  "name": "eco_client",
  "version": "0.0.3",
  "license": "MIT",
  "sideEffects": false,
  "main": "./src/index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "cp config/template.html src/ && export NODE_ENV=production && export ENABLE_BROTLI=true && preact build --config config/preact.config.js --no-esm --preload --sw --brotli true --template ./src/template.html",
    "serve": "sirv build --cors --single --port 3000 --http2 --cert ./fullchain.pem --key ./privkey.pem",
    "dev": "preact watch --sw false",
    "lint": "eslint src",
    "test": "jest",
    "bundle_watch": "webpack-bundle-analyzer --port 4200 build/stats.json"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "babel-loader": "^8.2.2",
    "compression-webpack-plugin": "6.1.1",
    "copy-webpack-plugin": "6.4.1",
    "css-loader": "5.2.7",
    "duplicate-package-checker-webpack-plugin": "3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.3",
    "file-loader": "^6.2.0",
    "fs": "^0.0.1-security",
    "html-minimizer-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "4.5.2",
    "idb": "^6.1.4",
    "if-env": "^1.0.4",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0",
    "imagemin-webpack-plugin": "2.4.2",
    "jest": "^26.6.3",
    "jest-preset-preact": "^4.0.2",
    "json-minimizer-webpack-plugin": "2.1.0",
    "mini-svg-data-uri": "1.2.3",
    "node-sass": "7.0.0",
    "optimize-plugin": "^1.0.0",
    "path": "^0.12.7",
    "preact-cli": "3.3.0",
    "preact-render-to-string": "5.1.19",
    "sass-loader": "10.1.1",
    "sirv": "^1.0.7",
    "sirv-cli": "^1.0.8",
    "strip-pragma-loader": "1.0.0",
    "style-loader": "2.0.0",
    "terser-webpack-plugin": "4.2.3",
    "unused-webpack-plugin": "^2.4.0",
    "url-loader": "4.1.1",
    "webpack": "4.46.0",
    "webpack-bundle-analyzer": "4.4.0",
    "webpack-merge": "5.7.3",
    "workbox-background-sync": "6.3.0",
    "workbox-cacheable-response": "6.3.0",
    "workbox-core": "6.3.0",
    "workbox-expiration": "6.3.0",
    "workbox-precaching": "6.3.0",
    "workbox-routing": "6.3.0",
    "workbox-strategies": "6.3.0",
    "xmlhttprequest": "^1.8.0"
  },
  "dependencies": {
    "cesium": "1.86.1",
    "firebase": "9.1.3",
    "nanoid": "3.1.29",
    "preact": "10.5.15",
    "preact-router": "^3.2.1",
    "react-dropdown-tree-select": "^2.5.1",
    "universal-cookie": "^4.0.4"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
