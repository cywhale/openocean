{
  "name": "ECO_client",
  "version": "0.0.2",
  "license": "MIT",
  "main": "./src/index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "cp config/template.html src/ && export NODE_ENV=production && preact build --config config/preact.config.js --no-esm --sw --template ./src/template.html",
    "serve": "sirv build --cors --single --port 3000 --http2 --cert ./fullchain.pem --key ./privkey.pem",
    "dev": "preact watch",
    "lint": "eslint src",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "preact",
    "ignorePatterns": [
      "build/"
    ]
  },
  "devDependencies": {
    "chalk": "^4.1.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-config-preact": "^1.1.0",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "if-env": "^1.0.4",
    "install": "^0.13.0",
    "jest": "^24.9.0",
    "jest-preset-preact": "^1.0.0",
    "node-sass": "^4.14.1",
    "path": "^0.12.7",
    "preact-cli": "^3.0.0-rc.6",
    "preact-render-spy": "^1.2.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "sass-loader": "^7.3.1",
    "sirv-cli": "^0.4.5",
    "strip-pragma-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.7",
    "url": "^0.11.0",
    "url-loader": "^4.1.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.0.9"
  },
  "dependencies": {
    "cesium": "^1.71.0",
    "preact": "^10.3.2",
    "preact-router": "^3.2.1"
  },
  "jest": {
    "preset": "jest-preset-preact",
    "setupFiles": [
      "<rootDir>/tests/__mocks__/browserMocks.js",
      "<rootDir>/tests/__mocks__/setupTests.js"
    ]
  }
}
